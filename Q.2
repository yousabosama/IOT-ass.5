#include <stdio.h>
int firstIndex(int arr[], int n, int x
){
    for(int i=0;i<n;i++)
        if(arr[i]==x) return i;
    return -1;
}
int main()
{
    int n,x;
    scanf("%d",&n);
    int a[n];
    for(int i=0;i<n;i++) scanf("%d",&a[i]);
    scanf("%d",&x);
    printf("%d", firstIndex(a,n,x));
    return 0;
}
