#include <stdio.h>
void swapArrays(int a[], int b[], int n)
{
    for(int i=0;i<n;i++)
{
        int temp=a[i];
        a[i]=b[i];
        b[i]=temp;
    }
}
int main()
{
    int n;
    scanf("%d",&n);
    int a[n],b[n];
    for(int i=0;i<n;i++) scanf("%d",&a[i]);
    for(int i=0;i<n;i++) scanf("%d",&b[i]);
    swapArrays(a,b,n);
    for(int i=0;i<n;i++) printf("%d ",a[i]);
    printf("\n");
    for(int i=0;i<n;i++) printf("%d ",b[i]);
    return 0;
}
